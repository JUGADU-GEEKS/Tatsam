{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "assets/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/style\\.css",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate",
          "content-type": "text/css"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  