{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "assets/**",
        "use": "@vercel/static"
      },
      {
        "src": "*.json",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/style\\.css",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate",
          "content-type": "text/css"
        }
      },
      {
        "src": "/(.*)\\.(?:jpg|jpeg|gif|png|svg|ico|mp3)$",
        "dest": "/$1.$2"
      },
      {
        "src": "/(.*)\\.js",
        "dest": "/$1.js",
        "headers": {
          "content-type": "application/javascript"
        }
      },
      {
        "src": "/(.*)\\.json",
        "dest": "/$1.json",
        "headers": {
          "content-type": "application/json"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  